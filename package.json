{
    "name": "chayns-emoji-picker",
    "version": "0.1.0",
    "license": "MIT",
    "author": "Leonhard Driesch",
    "main": "dist/index.js",
    "module": "dist/chayns-emoji-picker.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "spritesheets"
    ],
    "scripts": {
        "analyze": "size-limit --why",
        "build-storybook": "build-storybook -s ./spritesheets",
        "build": "tsdx build",
        "check-types": "tsc",
        "dev": "tsdx watch",
        "format": "prettier . --write --ignore-path .gitignore",
        "lint": "eslint ./src",
        "prepare": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks",
        "size": "size-limit",
        "storybook": "start-storybook -p 6006 -s ./spritesheets",
        "test": "jest",
        "prepublishOnly": "yarn build"
    },
    "resolutions": {
        "**/typescript": "^4.2.3"
    },
    "dependencies": {
        "@emotion/react": "^11.1.5",
        "@emotion/styled": "^11.1.5",
        "@reach/rect": "^0.14.0",
        "@react-aria/interactions": "^3.3.4",
        "framer-motion": "^4.0.3",
        "fuse.js": "^6.4.6",
        "react-focus-lock": "^2.5.0",
        "react-intersection-observer": "^8.31.0",
        "react-virtuoso": "^1.5.14",
        "twemoji-parser": "^13.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.14",
        "@babel/preset-env": "^7.13.12",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@emotion/babel-plugin": "^11.2.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@size-limit/preset-big-lib": "^4.10.2",
        "@storybook/addon-essentials": "^6.2.1",
        "@storybook/addon-links": "^6.2.1",
        "@storybook/addons": "^6.2.1",
        "@storybook/react": "^6.2.1",
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^13.1.1",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/twemoji-parser": "^12.1.0",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "chayns-components": "^4.21.1",
        "eslint": "^7.23.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.4.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^3.10.2",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "prettier-plugin-packagejson": "^2.2.10",
        "pretty-quick": "^3.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "shx": "^0.3.3",
        "size-limit": "^4.10.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.1.0",
        "typescript": "^4.2.3"
    },
    "peerDependencies": {
        "react": ">=16.8.0"
    },
    "engines": {
        "node": ">=10"
    },
    "size-limit": [
        {
            "path": "dist/chayns-emoji-picker.cjs.production.min.js",
            "limit": "150 KB"
        },
        {
            "path": "dist/chayns-emoji-picker.esm.js",
            "limit": "150 KB"
        }
    ]
